{
    "manifest_version": 2,
    "name": "N-ReportAssist",
    "version": "1.0.0",
    "description": "for secure.nnn.ed.jp/mypage",
    "author": "takonomura",
    "content_scripts": [
        {
            "matches": [
                "https://secure.nnn.ed.jp/mypage/report/pc/list/",
                "https://secure.nnn.ed.jp/mypage/report/pc/list/?*",
                "https://secure.nnn.ed.jp/mypage/report/pc/list/index",
                "https://secure.nnn.ed.jp/mypage/report/pc/list/index?*",
                "https://secure.nnn.ed.jp/mypage/result/pc/list/",
                "https://secure.nnn.ed.jp/mypage/result/pc/list/?*",
                "https://secure.nnn.ed.jp/mypage/result/pc/list/index",
                "https://secure.nnn.ed.jp/mypage/result/pc/list/index?*"
            ],
            "css": ["report-list.css"],
            "js": ["report-list.js"]
        },
        {
            "matches": [
                "https://secure.nnn.ed.jp/mypage/report/pc/list/unit?*",
                "https://secure.nnn.ed.jp/mypage/result/pc/list/unit?*"
            ],
            "js": ["unit-list.js"]
        }
    ],
    "permissions": [
        "https://secure.nnn.ed.jp/mypage/*"
    ],
    "update_url": "https://takonomura.github.io/N-ReportAssist/updates.xml"
}
